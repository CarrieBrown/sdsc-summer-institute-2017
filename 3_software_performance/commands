# Getting processor info
less /proc/cpuinfo
grep processor /proc/cpuinfo
grep 'physical id' /proc/cpuinfoÂ 

# Getting memory info
less /proc/meminfo

# Getting GPU info
nvidia-smi

# Getting cache info
cd /sys/devices/system/cpu
cd cpu0/cache
cd index0

# Getting SCSI device info
cat /proc/scsi/scsi

# Getting mounted file system
less /etc/mtab
df -h

# Getting network information
/sbin/ip link

# Getting OS and kernel info
uname -r
uname -o
uname -a
cat /etc/centos-release

# Getting an interactive node on Comet
srun --res=SI2017DAY1 --pty --nodes=1 --ntasks-per-node=24 -t 00:30:00 --wait=0 --export=ALL /bin/bash

# Compiling code for top example
icc -O3 -o lineq_top lineq_top.c -mkl


# 1st gprof example
ifort -pg -O3 -o gprof_ex gprof_ex.f
time ./gprof_ex 100000000
gprof gprof_ex gmon.out > profile_gp


# 2nd gprof example
icc -pg -O3 -o lineq_top lineq_top.c -mkl
time ./lineq_top 5000
gprof lineq_top gmon.out > profile_lineq



# Thread scaling example
icc -O3 -o thread_scaling thread_scaling.c -mkl
export OMP_NUM_THREADS=1
./lineq_mkl 3000
export OMP_NUM_THREADS=2
./lineq_mkl 3000
export OMP_NUM_THREADS=4
./lineq_mkl 3000
